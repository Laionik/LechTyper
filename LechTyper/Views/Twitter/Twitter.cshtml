@model List<LechTyper.Models.Tweet>
@{
    ViewBag.Title = "Twitter";
}
<div class="page-header">
    <div class="row">
        <div class="col-lg-12">
            <h1>@ViewBag.Title</h1>
        </div>
    </div>
</div>
<div class="bs-docs-section">
    <div class="row">
        <div class="col-lg-10">
            <p class="bs-component">
                <h3>Wczytane wpisy (Razem: @Model.Count())</h3>
                @{
                    foreach (var x in Model)
                    {
                        string link = "https://twitter.com/" + x.user_nick + "/status/" + x.post_id;
                        <span style="white-space: pre-line"><a href="@link">@x.user_nick</a></span>
                        <br />
                    }
                }
            </p>
        </div>
        @*<div class="col-lg-2">
                        <h3>Inne tagi</h3>
                        <ul>
                            @{
            <li><a href="instascore.somee.com">InstaScore</a></li>
                            }
                        </ul>
                    </div>*@
    </div>
</div>
