@using LechTyper.Resources
@model System.Web.Mvc.HandleErrorInfo

<!--PAGE TITLE-->
<div class="row">
    <div class="span12">
        <div class="page-header">
            <h1>@ResourceMain.Error</h1><br>
        </div>
    </div>
</div>
<!-- /. PAGE TITLE-->

<div class="alert alert-dismissible alert-danger">
    <h1 class="error">Błąd.</h1>
    <h2 class="error">Wystąpił błąd podczas przetwarzania Twojego polecenia. Skontaktuj się z administratorem.</h2>
    @Model.Exception.Message
</div>