@using LechTyper.Resources
@model List<LechTyper.Models.League>
<div class="page-header">
    <div class="row">
        <div class="col-lg-12">
            <h1>@ResourceMain.LeaguePromotions</h1>
        </div>
    </div>
</div>
<div class="bs-docs-section">
    <div class="row">
        <div class="col-lg-10">
            <p class="bs-component">
                <h3>Awanse i spadki</h3>
                <table class="table table-striped table-hover">
                    <thead>
                        <tr>
                            <th>@ResourceMain.Id</th>
                            <th>@ResourceMain.Division</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var player in Model)
                        {
                            <tr>
                                <td>@player.userId</td>
                                <td>@player.division</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </p>
        </div>
    </div>
</div>

